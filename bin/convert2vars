#!/bin/bash

APP_BIN_DIR=$(dirname ${0})
. ${APP_BIN_DIR}/setupenv.sh
export TOOL_BASE_DIR=$(readlink -f ${APP_BIN_DIR}/..)

# convert2vars.pyスクリプト(Python)を起動
(cd ${TOOL_BASE_DIR} > /dev/null; bin/convert2vars_cli.py "${@}")

if [ "${?}" == "0" ]; then
    exit ${STATUS_ALL_PASS}
else
    exit ${STATUS_SOME_FAIL}
fi
